%header#portfolio-header.main-header{"ng-controller" => "PortfolioController", "ng-init" => "init()",:style => "background-image: url(img/superheros/bg-code-" + "{{ activePage }}" + ".jpg)"}
  #portfolio-container-inner.main-header-inner
    .my-img.large-2.medium-3.columns
      %img{:src => "img/pixelme/pixel-me-" + "{{ activePage }}" + ".png"}

    .my-info.large-10.medium-9.columns
      %h1 {{ title }}

      %p.quote {{ quote }}

      %span.quotee {{ quotee }}

    %a.btn.btn-mini.btn-header.btn-portfolio{href: "https://www.behance.net/thomaswicker", target: "_blank"}
      View My Behance

%ng-include{:src => "'partials/nav.html'"}
%ul.subnav.portfolio{ 'role' => 'navigation', 'aria-label' => 'social networks navigation' }
  %ng-include{ :src => "'partials/subnav.html'" }

